#include <stdio.h>

int main()
  {
  int j = 0, i = 0, soma = 0, soma1 = 0;
  while(i < 200)
    {
    j = j % 3 ? j + 2 : j;
    if(j <= 100)
      {
      soma = soma + j;
      j++;
      }
    i = i % 5 ? i + 4 : i;
    if(i <= 200)
      {
      soma1 = soma1 + i;
      i++;
      }
    }
  printf("\nsomatorio dos divisiveis por 3 = %d\nsomatorio dos divisiveis por 5 = %d\n", soma, soma1);
  return 0;
  }
