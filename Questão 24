#include <stdio.h>

int main() 
  {
  int a = 1, b = 1, c = 1;
    
  for(c = 1; c <= 500; c++)
    {
    for(a = 1; a <= 500; a++)
      {
      for(b = 1; b <= 500; b++)
        {
        if(((a * a) + (b * b)) == (c * c))
          {
          printf("Cateto 1 = %d\nCateto 2 = %d\nHipotenusa = %d\n", a, b, c);    
          }
        }
      }
    }
  return 0;
  }
  
